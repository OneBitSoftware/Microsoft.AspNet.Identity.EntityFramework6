{
  "version": "3.0.1",
  "description": "An ASP.NET Core Identity 3.0 provider for Entity Framework 6",
  "authors": [ "Radi Atanasov" ],

  "packOptions": {
    "tags": [ "ASP.NET, aspnet, identity, entityframework" ],
    "repository": {
      "type": "git",
      "url": "git://github.com/OneBitSoftware/Microsoft.AspNet.Identity.EntityFramework6"
    }
  },

  "buildOptions": {
    "warningsAsErrors": true
  },

  "dependencies": {
    "Microsoft.AspNetCore.Identity": "1.0.0",
    "Microsoft.Extensions.DependencyInjection": "1.0.0",
    "Microsoft.Extensions.DependencyInjection.Abstractions": "1.0.0",
    "EntityFramework": "6.1.3"
  },

  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Data": ""
      }
    }
  }
}
